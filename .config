#
# Host: Odysseus
#
# serial console
#console com 1 9600.	

#
# device drivers
#
#      blk chr, or chr
!wt.
!mcd.
!as.

wd	0 3	(0x1f0 14).			# ide
fd	2 9 1 	(0x3f0 6 2).			# floppy
# as	4 13	(0x230).			# adaptec 1542 SCSI
# mcd	5 16	(0x300 10).			# mitsumi CDROM
com	8	(0x3f8 4 -1 -1 -1 1).		# 1 serial port
pccons	12	(0x3d0 1 -1 0xb8000 65536).	# console video
aux	14	(0x310 12).			# mouse port
lpt	15	(0x378 7).			# parallel port
npx	(0 13).					# numeric coprocessor ext.

# pseudo drivers
devtty	1.			# /dev/tty
swap	1 4.			# /dev/swap
mem	2.			# /dev/mem
pty	5 6	16.		# 8 pseudo ttys
log	7.			# /dev/log
fdesc	53.			# /dev/fd/*

# network interfaces
# ed 1	(0x280	3 -1 0xd8000 8192).       # smc ethernet
# ed 2	(0x250	5 -1 0xd8000 8192) (0x280	9 -1 0xd0000 8192).       # two ns ethernets
ed 1	(0x300	9 -1 0xd0000 8192).       # ne2000 ethernet

# line disciplines
termios	0.			# normal 8-bit character POSIX tty driver
slip	4.			# serial line ip

# filesystems
ufs	1.
nfs	2.
mfs	3.
msdos	4.
isofs	5.

# network protocol or communication domains
un 1.
inet 2.
