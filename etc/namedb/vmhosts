@		IN	SOA	odysseus.vm.	root.odysseus.vm.  (
					1.1     ;serial
					3600    ;refresh
					600     ;retry
					3600000;expire
					86400   ;minimum
					)
;name server for vm
		IN	NS	odysseus.vm.

localhost.vm.	IN	A	127.1
loopback	IN	CNAME	localhost.vm.

odysseus.vm.	IN	A	192.168.100.14
odysseus-vm	IN	CNAME	odysseus.vm.

boson.vm.	IN	A	192.168.100.1
boson-vm	IN	CNAME	boson.vm.

